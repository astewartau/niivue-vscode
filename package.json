{
  "name": "niivue",
  "displayName": "NiiVue",
  "description": "View NIfTI images (and a lot of other medical imaging formats)",
  "version": "1.3.0",
  "publisher": "KorbinianEckstein",
  "icon": "niivue_icon.png",
  "bugs": {
    "url": "https://github.com/niivue/niivue-vscode/issues",
    "email": "korbinian90+niivue@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/niivue/niivue-vscode"
  },
  "homepage": "https://github.com/niivue",
  "engines": {
    "vscode": "^1.70.0"
  },
  "license": "MIT",
  "categories": [
    "Visualization"
  ],
  "keywords": [
    "MRI",
    "NIfTI",
    "DICOM",
    "imaging",
    "medical",
    "neuroimaging",
    "neuroscience",
    "neuro",
    "brain",
    "fMRI",
    "DTI",
    "viewer"
  ],
  "extensionKind": [
    "workspace"
  ],
  "main": "./out/extension.js",
  "browser": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "niivue.openWebLink",
        "title": "NiiVue: Open Web Link"
      },
      {
        "command": "niiVue.compareFromExplorer",
        "title": "NiiVue: Compare",
        "enablement": "listMultiSelection"
      },
      {
        "command": "niiVue.openFromExplorer",
        "title": "NiiVue: Open",
        "enablement": "!listMultiSelection"
      }
    ],
    "customEditors": [
      {
        "viewType": "niiVue.default",
        "displayName": "NiiVue",
        "selector": [
          {
            "filenamePattern": "*.dcm"
          },
          {
            "filenamePattern": "*.nii"
          },
          {
            "filenamePattern": "*.nii.gz"
          },
          {
            "filenamePattern": "*.mih"
          },
          {
            "filenamePattern": "*.mif"
          },
          {
            "filenamePattern": "*.mif.gz"
          },
          {
            "filenamePattern": "*.nhdr"
          },
          {
            "filenamePattern": "*.nrrd"
          },
          {
            "filenamePattern": "*.mhd"
          },
          {
            "filenamePattern": "*.mha"
          },
          {
            "filenamePattern": "*.mgh"
          },
          {
            "filenamePattern": "*.mgz"
          },
          {
            "filenamePattern": "*.v"
          },
          {
            "filenamePattern": "*.v16"
          },
          {
            "filenamePattern": "*.vmr"
          },
          {
            "filenamePattern": "*.mz3"
          },
          {
            "filenamePattern": "*.gii"
          }
        ]
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "niiVue.compareFromExplorer",
          "when": "listMultiSelection"
        },
        {
          "command": "niiVue.openFromExplorer",
          "when": "!listMultiSelection"
        }
      ]
    }
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "activationEvents": [
    "*"
  ],
  "scripts": {
    "install:all": "yarn install && cd niivue && yarn install",
    "start:webview": "cd niivue && yarn run start",
    "build:webview": "cd niivue && yarn run build",
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "@vscode/test-web": "^0.0.47",
    "buffer": "^6.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "glob": "^8.1.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {}
}
