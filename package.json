{
  "name": "niivue",
  "displayName": "NiiVue",
  "description": "View NIfTI images (and a lot of other formats)",
  "version": "0.1.2",
  "publisher": "KorbinianEckstein",
  "bugs": {
    "url": "https://github.com/niivue/niivue-vscode/issues",
    "email": "korbinian90+niivue@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/niivue/niivue-vscode"
  },
  "homepage": "https://github.com/niivue",
  "engines": {
    "vscode": "^1.70.0"
  },
  "license": "MIT",
  "categories": [
    "Visualization"
  ],
  "preview": true,
  "keywords": [
    "MRI",
    "NIfTI",
    "DICOM",
    "imaging"
  ],
  "main": "./dist/extension-vscode.js",
  "browser": "./dist/extension-vscode.js",
  "contributes": {
    "commands": [
      {
        "command": "niiVue.open",
        "title": "NiiVue: Open Image"
      },
      {
        "command": "niiVue.openLink",
        "title": "NiiVue: Open Link"
      },
      {
        "command": "niiVue.openLocal",
        "title": "NiiVue: Open Local File"
      }
    ],
    "customEditors": [
      {
        "viewType": "niiVue.default",
        "displayName": "NiiVue",
        "selector": [
          {
            "filenamePattern": "*.nii"
          },
          {
            "filenamePattern": "*.nii.gz"
          },
          {
            "filenamePattern": "*.dcm"
          },
          {
            "filenamePattern": "*.mih"
          },
          {
            "filenamePattern": "*.mif"
          },
          {
            "filenamePattern": "*.nhdr"
          },
          {
            "filenamePattern": "*.nrrd"
          },
          {
            "filenamePattern": "*.mhd"
          },
          {
            "filenamePattern": "*.mha"
          },
          {
            "filenamePattern": "*.mgh"
          },
          {
            "filenamePattern": "*.mgz"
          },
          {
            "filenamePattern": "*.v"
          },
          {
            "filenamePattern": "*.v16"
          },
          {
            "filenamePattern": "*.vmr"
          },
          {
            "filenamePattern": "*.head"
          }
        ]
      }
    ]
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "activationEvents": [
    "*"
  ],
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "prepackage": "copyfiles node_modules/@niivue/niivue/dist/niivue.umd.js media",
    "package": "webpack --mode production --devtool hidden-source-map",
    "webpack:dev": "webpack --mode development --watch",
    "webpack:prod": "webpack --mode production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@niivue/niivue": "^0.36.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "copy-webpack-plugin": "^11.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "style-loader": "^3.3.3",
    "css-loader": "^6.8.1",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "yarn": "^1.22.19"
  }
}
